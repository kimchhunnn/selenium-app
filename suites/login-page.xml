<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM '<a href="http://testng.org/testng-1.0.dtd">http://testng.org/testng-1.0.dtd</a>'>
<suite name="Login Page"> 
	<test name = "Login Page UI - chrome"> 
		<parameter name="browser" value="chrome"/>
		<classes>
		      <class name = "com.storybook_app.login.Page_Login_UI"/>
		</classes> 
	</test> 
	
	<test name = "Login Page - chrome"> 
		<parameter name="browser" value="chrome"/>
		<classes> 
		      <class name = "com.storybook_app.login.Page_Login"/> 
		</classes> 
	</test> 

	<test name = "Login Page UI - Firefox"> 
		<parameter name="browser" value="firefox"/>
		<classes>
		      <class name = "com.storybook_app.login.Page_Login_UI"/>
		</classes> 
	</test>
	
	<test name = "Login Page - Firefox"> 
		<parameter name="browser" value="firefox"/>
		<classes> 
		      <class name = "com.storybook_app.login.Page_Login"/> 
		</classes> 
	</test>

</suite>